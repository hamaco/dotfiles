{
    "title": "App switcher",
    "rules": [
        {
            "description": "cmd + number to app switch",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "1",
                        "modifiers": {
                            "mandatory": [
                                "command"
                            ]
                        }
                    },
                    "to": [
                        {
                            "shell_command": "open -a Vivaldi"
                        }
                    ]
                },
