if exists ruby && exists hub; then
  fpath=($(dirname $0)/completions $fpath)
  function git() { hub "$@" }
fi

alias g="git"
alias gist="gist -cop"

alias gst="git status -sb"

alias gs="git svn"
alias gsr="git svn rebase"
alias gsd="git svn dcommit"

alias ggpush="git push origin \$(git current-branch)"
