snippet php
	<?php
	${1}

snippet cls
	class ${1:className}
	{
		${2}
	}

snippet fn
	function ${1}(${2})
	{
		${3}
	}

snippet pfn
	public function ${1:FuncName}(${2:arguments})
	{
		${3}
	}

snippet ife
	if (${1}) {
		${2}
	} else {
		${3}
	}

snippet ve
	var_dump(${1});exit;

# vim: noexpandtab ts=2 sw=2
