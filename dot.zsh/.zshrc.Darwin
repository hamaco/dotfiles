local COMMAND=""
local COMMAND_TIME=""
precmd() {
	if [ "$COMMAND_TIME" -ne "0" ] ; then
		local d=`date +%s`
		d=`expr $d - $COMMAND_TIME`
		if [ "$d" -ge "30" ] ; then
			COMMAND="$COMMAND "
			growlnotify -t "${${(s: :)COMMAND}[1]}" -m "$COMMAND"
		fi
	fi
	COMMAND="0"
	COMMAND_TIME="0"
}
preexec () {
	COMMAND="${1}"
	COMMAND_TIME=`date +%s`
}
