local COMMAND=""
local COMMAND_TIME=""
precmd() {
	case "$COMMAND" in
		v*|tail*)
			;;
		*)
			if [ "$COMMAND_TIME" -ne "0" ] ; then
				local d=`date +%s`
				d=`expr $d - $COMMAND_TIME`
				if [ "$d" -ge "30" ] ; then
					COMMAND="$COMMAND "
					growlnotify -t "${${(s: :)COMMAND}[1]}" -m "$COMMAND" -s
				fi
			fi
			;;
	esac
	COMMAND="0"
	COMMAND_TIME="0"
}
preexec () {
	COMMAND="${1}"
	COMMAND_TIME=`date +%s`
}
