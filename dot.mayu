include "104.mayu"

keymap Global
	# Applications as Windows
	mod Windows      += Applications
	key *Applications = *RWindows

	# CapsLock as Control
	mod Control  += CapsLock
	key *CapsLock = *LControl

	# Sands
	mod shift += !!!Space

	key F1 = &Ignore
	key A-F1 = F1

	# Windows-q as Alt-F4
	key W-q = A-F4
