snippet css
	<link rel="stylesheet" type="text/css" href="${1}" />

snippet js
	<script type="text/javascript" src="${1}"></script>

snippet jquery
	<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.7.2/jquery.min.js"></script>

snippet pager
	<partial name="pager" />
	<foreach from="$paginator->results" value="${1}">
		${2}
	</foreach>
	<partial name="pager" />
